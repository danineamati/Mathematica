Cell[BoxData[{
 RowBox[{"Clear", "[", 
  RowBox[{"ayu", ",", "byu", ",", "shearU"}], "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"q1u", "[", "x_", "]"}], ":=", 
  RowBox[{"simpleForceDist", "[", 
   RowBox[{"0", ",", "3", ",", "0", ",", "300", ",", "x"}], 
   "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"q2u", "[", "x_", "]"}], ":=", 
  RowBox[{"simpleForceDist", "[", 
   RowBox[{"3", ",", "5", ",", "300", ",", "300", ",", "x"}], 
   "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"q3u", "[", "x_", "]"}], ":=", 
  RowBox[{"simpleForceDist", "[", 
   RowBox[{"5", ",", "10", ",", "400", ",", "400", ",", "x"}], 
   "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"aMagu", "=", 
   FractionBox["aMag", 
    RowBox[{"Quantity", "[", 
     RowBox[{"1", ",", "\"\<Pounds\>\""}], "]"}]]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"bMagu", "=", 
   FractionBox["bMag", 
    RowBox[{"Quantity", "[", 
     RowBox[{"1", ",", "\"\<Pounds\>\""}], "]"}]]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"ayu", "[", "x_", "]"}], ":=", 
  RowBox[{"reactionPiece", "[", 
   RowBox[{"aMagu", ",", "0", ",", "x"}], "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"byu", "[", "x_", "]"}], ":=", 
   RowBox[{"reactionPiece", "[", 
    RowBox[{"bMagu", ",", "10", ",", "x"}], "]"}]}], "\[IndentingNewLine]", 
  "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{
    RowBox[{"q1u", "[", "x", "]"}], ";", "\[IndentingNewLine]", 
    RowBox[{"q2u", "[", "x", "]"}], ";", "\[IndentingNewLine]", 
    RowBox[{"q3u", "[", "x", "]"}], ";", "\[IndentingNewLine]", 
    RowBox[{"ayu", "[", "x", "]"}], ";", "\[IndentingNewLine]", 
    RowBox[{"byu", "[", "x", "]"}], ";"}], "*)"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"shearU", "[", "x_", "]"}], ":=", 
  RowBox[{
   RowBox[{"ayu", "[", "x", "]"}], "+", 
   RowBox[{"Integrate", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"q1u", "[", "s", "]"}], "+", 
      RowBox[{"q2u", "[", "s", "]"}], "+", 
      RowBox[{"q3u", "[", "s", "]"}]}], ",", 
     RowBox[{"{", 
      RowBox[{"s", ",", "0", ",", "x"}], "}"}], ",", 
     RowBox[{"Assumptions", "\[Rule]", 
      RowBox[{"x", " ", "\[Element]", " ", "Reals"}]}]}], "]"}], "+", 
   RowBox[{"byu", "[", "x", "]"}]}]}], "\[IndentingNewLine]", 
 RowBox[{"Plot", "[", 
  RowBox[{
   RowBox[{"shearU", "[", "x", "]"}], ",", 
   RowBox[{"{", 
    RowBox[{"x", ",", "0", ",", "11"}], "}"}]}], "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"bendingU", "[", "x_", "]"}], ":=", 
  RowBox[{"Integrate", "[", 
